{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^components/(.*)$",
    "^pages/(.*)$",
    "^ui/(.*)$",
    "^hooks/(.*)$",
    "^widgets/(.*)$",
    "^helpers/(.*)$",
    "^services/(.*)$",
    "^assets/(.*)$",
    "^utils/(.*)$",
    "^config/(.*)$",
    "^store/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
